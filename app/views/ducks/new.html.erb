<%= form_for @duck do |f| %>
    <%= f.label :name %><br>
    <%= f.text_field :name %><br>

    <%= f.label :description %><br>
    <%= f.text_field :description %><br>

    <%= f.label :student_id, name="Student" %><br>
    <%= f.collection_select :student_id, Student.all, :id, :name %><br>

    <%= f.submit %>
<% end %>