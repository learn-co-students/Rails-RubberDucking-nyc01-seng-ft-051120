<h1> Edit <%= @student.name %>!</h1>

<% if flash[:error] %>
<ul>
<% flash[:error].each do |error| %>
  <li><%= error %></li>
  <%end%>
  </ul>
<%end%>

<%= form_for @student do |f| %>
<%= f.label :name %>
<%= f.text_field :name %><br>
<%= f.label :mod %>
<%= f.text_field :mod %><br>
<%= f.submit%>
<%end%>